[alias]
	grog = log --graph --abbrev-commit --decorate --all \n--format=format:\"%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(dim white) - %an%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n %C(white)%s%C(reset)\"
	cm = commit -m
	aa = add .
	ps = push
	pl = pull
	st = status
	co = checkout
	br = branch
	df = diff
	commend = commit --amend --no-edit
	hlog = log --all --graph --pretty='format:%C(blue)%ar%C(reset) %C(yellow)%h%C(reset) %C(blue)%s%C(reset) %C(magenta)[%an]%C(reset)%C(green)%d%C(reset)'
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset'
